{
  "name": "generator-unchained",
  "version": "1.0.1",
  "description": "Your workflow - now unchained.",
  "homepage": "https://github.com/unchained/generator-unchained",
  "author": {
    "name": "UNCHAINED studio",
    "email": "hello@unchained.studio",
    "url": "https://unchained.studio"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator",
    "unchained",
    "generator",
    "express",
    "gulp",
    "webpack",
    "sass",
    "MVC",
    "babel",
    "ES6"
  ],
  "devDependencies": {
    "yeoman-test": "^1.6.0",
    "yeoman-assert": "^3.0.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "dedent-js": "^1.0.1",
    "yeoman-generator": "^1.0.0",
    "yosay": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unchained/generator-unchained.git"
  },
  "license": "MIT"
}
